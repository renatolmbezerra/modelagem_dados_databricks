    "UPDATE workshop_modelagem.silver.dim_customer_scd\n",
    "SET row_hash = sha2(concat_ws('||',\n",
    "  coalesce(customer_name,''),\n",
    "  coalesce(lower(email),''),\n",
    "  coalesce(city,''),\n",
    "  coalesce(upper(trim(state)),'')\n",
    "), 256)\n",
    "WHERE row_hash IS NULL;"
